<table class="table">
  <thead>
    <tr class="bg-dark text-white">
      <th scope="col">Nombre</th>
      <th scope="col">Autor</th>
      <th scope="col">precio</th>
      <th scope="col">Foto</th>
    </tr>
  </thead>
  <tbody>
    {{#each arrObjetos}}
    <tr class="bg-secondary text-white">
        <th >{{nombre}}</td>
        <td>{{autor}}</td>
        <td>{{precio}}</td>
        <td><img src="{{foto}}" width="60px"></td>
    </tr>
    {{/each}}
 
  </tbody>
</table>